        button(id="out") - 😀
      p
        span(id="visits")
        span  👣
    br
    fieldset
      legend Könummer
      button(id="que") + 😐
      span 
      span(id="lastInQue")
      span  (nytt) 
      p
        button(id="call") - 😐
        span 
        span(id="firstInQue")
        span  (kallat) 
      p ⏳ 
        span(id="queTime") 
        span  min
    p
      a(href='/logout') Logga ut
    script(src="client.js")
